PORT=3000
NODE_ENV=development
RATE_LIMIT_DELAY=2000
CACHE_TTL=86400000
MAX_FILE_SIZE=5242880

# Domain Provider: 'godaddy' o 'whois'
DOMAIN_PROVIDER=whois

# GoDaddy API
# Obtén tus keys en: https://developer.godaddy.com/keys
# 1. Crea una cuenta en GoDaddy Developer
# 2. Ve a "API Keys" y crea una nueva key de PRODUCCIÓN
# 3. Copia el API Key y API Secret aquí
GODADDY_API_KEY=your_api_key_here
GODADDY_API_SECRET=your_api_secret_here
